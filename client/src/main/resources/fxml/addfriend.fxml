<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane maxHeight="600.0" maxWidth="300.0" minHeight="442.0" minWidth="300.0" prefHeight="442.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.htwsaar.vs.msgr.client.controllers.AddFriendController">
   <children>
      <VBox prefHeight="442.0" prefWidth="300.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
             <Label prefHeight="35.0" prefWidth="232.0" text="Neuer Kontakt hinzufügen">
               <font>
                  <Font name="DejaVu Sans Bold" size="14.0" />
               </font>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Label>
             <TextField fx:id="searchFriend" onKeyPressed="#pressEnter" prefHeight="25.0" prefWidth="256.0" promptText="Suche User">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" />
               </VBox.margin>
            </TextField>
            <ListView fx:id="findUsers" prefHeight="300.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
            <HBox alignment="CENTER" prefHeight="15.0" prefWidth="300.0" spacing="40.0" VBox.vgrow="NEVER">
               <children>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButton" onMousePressed="#okButton" text="Übernehmen" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButton" onMousePressed="#cancelButton" text="Abbrechen" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
